<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.boot.web.dao.JsonPMapper">

	<select id="testJsonp" resultType="com.boot.web.entity.JsonPTo">
		SELECT
		param_key,
		param_name,
		param_desc,
		param_value,
		param_unit,
		is_inactive
		FROM sys_param
		ORDER BY param_key
	</select>

</mapper>